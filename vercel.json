{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/generate",
      "dest": "/api/generate.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
} 